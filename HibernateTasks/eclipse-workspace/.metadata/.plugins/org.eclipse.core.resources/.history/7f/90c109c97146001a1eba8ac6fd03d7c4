
public class InheritenceExample {
	InheritenceExample(){
		System.out.println("Base Class");
	}
	public void add() {
		int a=10;
	}
}
class ChildClass extends InheritenceExample{
	int b=20;
	static int sum=0;
	public static void main(String args[]) {
		ChildClass cc = new ChildClass();
		sum = b+ cc.a;
	}
	
}
