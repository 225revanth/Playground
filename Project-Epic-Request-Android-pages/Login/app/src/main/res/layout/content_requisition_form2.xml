<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".RequisitionForm2"
    tools:showIn="@layout/app_bar_requisition_form2">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/fdfdfdbackground"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:text="REQUISITION"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/dashedline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16">

                <TextView
                    android:id="@+id/page1"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:background="@drawable/whitecircle"
                    android:gravity="center"
                    android:text="1"
                    android:textColor="#D3D3D3"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/page2"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/bluecircle"
                    android:gravity="center"
                    android:text="2"
                    android:textColor="#FFFFFF"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toEndOf="@+id/page1"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/page3"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/whitecircle"
                    android:gravity="center"
                    android:text="3"
                    android:textColor="#D3D3D3"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toEndOf="@+id/page2"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/page4"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/whitecircle"
                    android:gravity="center"
                    android:text="4"
                    android:textColor="#D3D3D3"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toEndOf="@+id/page3"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/page5"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/whitecircle"
                    android:gravity="center"
                    android:text="5"
                    android:textColor="#D3D3D3"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toEndOf="@+id/page4"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/page6"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/whitecircle"
                    android:gravity="center"
                    android:text="6"
                    android:textColor="#D3D3D3"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toEndOf="@+id/page5"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>


    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>